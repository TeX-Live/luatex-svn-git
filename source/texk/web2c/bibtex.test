rm -f tests/xexampl.*

cp $srcdir/tests/exampl.aux tests/xexampl.aux

TEXMFCNF=$srcdir/../kpathsea \
  BSTINPUTS=$srcdir/tests \
  BIBINPUTS=$srcdir/tests \
  ./bibtex tests/xexampl || exit 1
diff $srcdir/tests/exampl.bbl tests/xexampl.bbl || exit 2

